<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>loginController</servlet-name>
        <servlet-class>Login.loginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerController</servlet-name>
        <servlet-class>RegisterPack.registerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>URLController</servlet-name>
        <servlet-class>Controller.URLController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>Logout.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewEventController</servlet-name>
        <servlet-class>eventPack.NewEventController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eventController</servlet-name>
        <servlet-class>eventPack.eventController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newEnrollmentController</servlet-name>
        <servlet-class>enroll.newEnrollmentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eventEnrollment</servlet-name>
        <servlet-class>enroll.eventEnrollment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrganiserEventController</servlet-name>
        <servlet-class>eventPack.OrganiserEventController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginController</servlet-name>
        <url-pattern>/loginURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerController</servlet-name>
        <url-pattern>/registerURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>URLController</servlet-name>
        <url-pattern>/URLController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewEventController</servlet-name>
        <url-pattern>/NewEventURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eventController</servlet-name>
        <url-pattern>/eventController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newEnrollmentController</servlet-name>
        <url-pattern>/newEnrollmentController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eventEnrollment</servlet-name>
        <url-pattern>/eventEnrollment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrganiserEventController</servlet-name>
        <url-pattern>/OrganiserEventController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>homePage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
